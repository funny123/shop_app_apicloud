		//				if (funcName) {
		//					H.$openWin(funcName, funcName + '.html');
		//				}
		//			});
		//		});
		apiready = function() {
		    
			api.parseTapmode();
			$api.byId('header_img').src = $api.getStorage('header_img');
			if ($api.getStorage('member') == 'underfined') {
				$api.byId('member').innerHTML = '登录';
			} else {
				$api.byId('member').innerHTML = $api.getStorage('member');
			}
			var user = $api.getStorage('user');
			if (user) {
				return;
			} else {
				if (api.systemType == "ios") {
					var times = 0;
				} else {
					var times = 300;
				}
				//			api.closeFrame({
				//          });
				goLogin('../login/login_head');
				//			api.openWin({
				//				name : 'login',
				//				url : '../login/login_head.html',
				//				delay : times
				//			});
			}
		}
	</script>
</html>