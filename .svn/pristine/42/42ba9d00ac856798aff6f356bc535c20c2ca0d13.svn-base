	<script type="text/javascript">
		function openGoods() {
			openWinto('goods_list_head', 'goods/goods_list_head');
		}

		Zepto(function() {
			$("#leftbox").on("tap", "li", function() {
				$('#leftbox li').removeClass('active');
				$(this).addClass('active');
			});
		});
		apiready = function() {
			api.ajax({
				url : 'http://www.ghuog.com/yii2_shop/api/web/app/v1?job=gettype',
				method : 'get',
				data : {
					values : {
						//						name : 'haha'
					},
					files : {
						//						file : 'fs://a.gif'
					}
				}
			}, function(ret, err) {
				if (ret) {
					//					var app = new Vue({
					//						el : '#list',
					//						data : ret.data
					//					})
					for (var i = 0; i <= ret.data.length - 1; i++) {
						var html = '<li class="aui-list-view-cell aui-img aui-counter-list" tapmode onclick="openWinToType('+ret.data[i].clsno+');">';
						html = html + '<img class="aui-img-object aui-pull-left" src="' + ret.data[i].images + '" data-echo="' + ret.data[i].images + '">';
						html = html + '<div class="aui-img-body">';
						//						html = html + '<div class="aui-text-default aui-ellipsis-1"> 茅山榴莲王 越南进口越南进口越南进口</div>';
						html = html + '<span class="aui-text-default aui-ellipsis-1" style="padding-top=30px;">' + ret.data[i].clsname + '</span>';
						html = html + '<div class="aui-counter-box">';
						//						html = html + '<div class="aui-pull-left aui-font12">关注时间: 2015-06-20</div>';
						html = html + '<div class="aui-text-default aui-pull-right">';
						html = html + ' <i class="aui-iconfont aui-icon-delete aui-text-FF9800n"></i>';
						html = html + '</div>';
						html = html + '</div>';
						html = html + '</div>';
						html = html + ' </li>';
						$api.append($api.byId('list'), html);
					}
				} else {
					api.alert({
						msg : JSON.stringify(err)
					});
				}
			});
			api.parseTapmode();
		}
