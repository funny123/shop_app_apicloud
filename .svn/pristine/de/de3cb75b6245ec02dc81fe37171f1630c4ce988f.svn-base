/* 询价单管理 */
.article {
    font-size: 1.4rem;
    width: 100%;
}
.article .wrap {
    padding-bottom: 6rem;
    width: 100%;
    position: relative;
    top: 5.8rem;
    border-top: 0.1rem solid #eaeaea;
}
/** 照片或拍照 */
.article .wrap .photo {
    margin: 0.6rem 0rem 0rem 0rem;
    padding-bottom: 0.7rem;
    width: 100%;
    height: 9rem;
    border-bottom: 0.1rem solid #eaeaea;
}
.article .wrap .photo .shrink {
    text-indent: 0.4rem;
}
.article .wrap .photo .shrink div {
    padding: 0rem;
    margin: 0rem;
    width: 30%;
    height: 9rem;
    float: left;
}
.article .wrap .photo .shrink div img{
    margin-left: -0.4rem;
    width: 100%;
    border: none;
}
.article .wrap .photo .shrink div span {
    margin: 2.4rem 0rem 0rem 0rem;
    width: 4.5rem;
    height: 4.2rem;
    background-size: 4.5rem 4.2rem;
}
.article .wrap .photo .shrink div:nth-child(1) div {
    margin-left: 1rem;
    padding: 0rem;
    width: 9rem;
    height: 9rem;
    background-image: url("../image/nopic.png");
    background-size: 9rem;
    overflow: hidden;
    border: 0.1rem solid #dddddd;
}
.article .wrap .photo .shrink div:nth-child(2) span {
    margin-left: 4rem;
    background-image: url("../image/product_add_picture.png");
}
.article .wrap .photo .shrink div:nth-child(3) span {
    background-image: url("../image/product_add_album.png");
}

/** 列表 */
.article .wrap .choose {
    margin: 0.4rem 0rem 0rem 0rem;
    padding-bottom: 0.4rem;
    width: 100%;
    height: 3.4rem;
    line-height: 3.4rem;
    border-bottom: 0.1rem solid #eaeaea;
}
.article .wrap .choose label {
    width: 11rem;
    float: left;
    text-indent: 1rem;
}
.article .wrap .choose input {
    width: 20rem;
    height: 3.2rem;
    border: 0rem;
}
.article .wrap .choose select {
    padding: 0.4rem 0.6rem;
}
.article .wrap .choose input:focus {
    outline:none !important;
}
/*** 条形码扫描 */
.article .wrap .inquiry input{
    width: 16rem;
}
.article .wrap .inquiry span {
    padding: 0.1rem 1rem;
    line-height: 2.6rem;
    border-radius: 0.4rem;
    border: 0.1rem solid #eaeaea;
}
/*** 收货人信息 */
.article .wrap .thread {
    margin: 0rem 0rem 0rem 0rem;
    padding-top: 0.8rem;
    width: 100%;
    height: 3.4rem;
    line-height: 3.4rem;
    color: #b1b1b1;
    font-size: 1.6rem;
    text-indent: 1rem;
    background-color: #f0f0f0;
}
/*** 提交 */
.article .wrap button {
    width: 100%;
    height: 4.3rem;
    text-align: center;
    line-height: 4.3rem;
    color: #ffffff;
    font-size: 1.6rem;
    position: fixed;
    bottom: 0rem;
    background-color: #0b923b;
}


/** 我的询价单 */
.article .list {
    padding-bottom: 2rem;
    width: 100%;
    position: relative;
    top: 5.8rem;
    border-top: 0.1rem solid #eaeaea;
}
.article .list ul {
    margin: 0rem;
    padding: 0rem;
    width: 100%;
    height: 7rem;
    line-height: 3rem;
    border-bottom: 0.1rem solid #eaeaea;
}
.article .list ul li {
    margin: 0.8rem 0rem -1rem 0rem;
    padding: 0rem;
    font-size: 1.2rem;
}
.article .list ul li label {
    padding: 0rem 0.2rem 0rem 0rem;
    float: left;
    text-indent: 1.18rem;
}
.article .list ul li span {
    float: left;
}
.article .list ul li:nth-child(1) {
    width: 65%;
    float: left;
    text-indent: 1.18rem;
}
.article .list ul li:nth-child(3) {
    width: 65%;
    float: left;
}
.article .list ul li:nth-child(1) span, .list ul li:nth-child(3) span {
    width: 16rem;
    height: 3rem;
    overflow: hidden;
}
.article .list ul li:nth-child(2), .list ul li:nth-child(4) {
    width: 35%;
    float: left;
}
.article .list ul li:nth-child(2) span, .list ul li:nth-child(4) span {
    width: 7.6rem;
    height: 3rem;
    overflow: hidden;
}
/** 询价单详情 */
.article .details {
    position: relative;
    top: 1rem;
}