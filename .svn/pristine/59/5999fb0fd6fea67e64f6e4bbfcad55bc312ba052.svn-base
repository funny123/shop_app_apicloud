/* 供应商管理 */
.article {
    font-size: 1.4rem;
    width: 100%;
}
/** 头部 */
.article .four {
    margin: 1rem 0rem 0rem -15.8rem;
    width: 31.5rem;
    font-size: 1.3rem;
}
.article .four div {
    width: 7.8rem;
}
.article .four a {
    width: 7.8rem;
    border-right: 0.1rem solid #0b923b;
}
.article .four div:nth-last-child(1) {
    border: 0rem;
}
.article .four a:nth-last-child(1) {
    border: 0rem;
}


/** 添加供应商 */
.article .wrap {
    width: 100%;
    position: relative;
    top: 5.8rem;
    border-top: 0.1rem solid #eaeaea;
}
.article .wrap .choose {
    margin: 0.4rem 0rem 0rem 0rem;
    padding-bottom: 0.4rem;
    width: 100%;
    height: 3.6rem;
    line-height: 3.6rem;
    border-bottom: 0.1rem solid #eaeaea;
}
.article .wrap label {
    width: 11rem;
    text-indent: 1rem;
}
.article .wrap input {
    width: 20rem;
    height: 3.2rem;
    border: 0rem;
}
.article .wrap input:focus {
    outline:none !important;
}
.article .wrap select {
    padding: 0.4rem 0.6rem;
}
.article .wrap button {
    width: 100%;
    height: 4.3rem;
    text-align: center;
    line-height: 4.3rem;
    color: #ffffff;
    font-size: 1.6rem;
    position: fixed;
    bottom: 0rem;
    background-color: #0b923b;
}
/** 供应商列表 */
.article .listing label{
    width: 16rem !important;
    text-indent: 1.4rem;
}
/** 编辑供应商 */
.article .edit {
    top: 1rem;
}
/*** 确认 */
.article .edit .submit {
    width: 100%;
    height: 4.3rem;
    position: fixed;
    bottom: 0rem;
    font-size: 1.6rem;
}
.article .edit .submit div {
    text-align: center;
    line-height: 4.3rem;
    color: #ffffff;
    float: left;
}
.article .edit .submit div:nth-child(1) {
    width: 40%;
    background-color: #718077;
}
.article .edit .submit div:nth-child(2) {
    width: 60%;
    background-color: #00a91c;
}


/*** 列表 */
.article .listing .list {
    position: relative;
    top: 0rem;
    border-top: 0rem;
}
/** 新增分类 */
.article .wrap .add_classify {
    border-bottom: 0.1rem solid #eaeaea;
}
/** 供应商分类列表 */
.article .list {
    padding-bottom: 2rem;
    width: 100%;
    position: relative;
    top: 5.8rem;
    border-top: 0.1rem solid #eaeaea;
}
/** 列表 */
.article .list ul {
    margin: 0rem;padding: 0rem;
    width: 100%;
}
.article .list ul li {
    margin: 0rem;
    padding: 0rem;
    width: 100%;
    height: 3.6rem;
    font-size: 1.4rem;
    line-height: 3.6rem;
    border-bottom: 0.1rem solid #eaeaea;
}
.article .list ul li div {
    width: 88%;
    height: 3.6rem;
    float: left;
    text-indent: 1.4rem;
    overflow: hidden;
}
.article .list ul li span {
    width:12%;
    color: #c0c0c0;
    text-align: center;
}