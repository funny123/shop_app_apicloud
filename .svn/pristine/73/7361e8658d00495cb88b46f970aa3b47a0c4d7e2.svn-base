		*{ margin:0; padding:0;border:0;}
		html{-webkit-text-size-adjust:100%;-ms-text-size-adjust:100%;overflow-y:scroll;font-size:20px}
		html *{outline:0;-webkit-text-size-adjust:none;-webkit-tap-highlight-color:rgba(0,0,0,0);}
		body{font-family:Tahoma, Helvetica, Arial, 宋体, sans-serif;color:#323232;background-color:#ffffff;font-weight:400;}
		ul,ol,ul li,ol li{ list-style-type:none;}
		a{color:#323232;display:inline-block;text-decoration:none;outline:none;}
		a:hover{ color:#e60000;text-decoration:none;}
		a:focus {outline:none;}
		clear{clear:both; font-size:0px; height:0.05rem;; overflow:hidden;}
		input{-webkit-appearance : none ;}/*解决iphone safari上的圆角问题*/
		input,img{vertical-align:middle;border:0;}
		input,textarea{outline:0}
		input,select,textarea{font-size:100%;}
		table{border-collapse:collapse;border-spacing:0;}
		*, *:before, *:after {box-sizing: inherit;}
		html{-webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box;}
		
