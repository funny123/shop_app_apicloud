		// 获取信息
		function headList(page) {
			api.showProgress({
				title : '加载中',
				modal : false
			});
			api.ajax({
				url : 'http://www.ghuog.com/yii2_shop/api/web/app/v1?job=get_recommend_goods&branch_no=5',
				method : 'post',
				//			data: {
				//				values: {
				//					classid: classid,
				//					page: _page
				//				}
				//			}
			}, function(ret, err) {
				if (ret) {
					//					alert(JSON.stringify(ret.data));
					//					alert(ret.data.length);
					for (var i = 0; i <= ret.data.length - 1; i++) {
						//						var str = '<li class="aui-list-item" tapmode onclick="fnOpenContent(\'' + ret.data[i].no + '\')">';
						var str = ' <li class="aui-list-view-cell aui-img aui-col-xs-6" tapmode onclick="openGoods(\'./goods/goods_content\',\'' + ret.data[i].no + '\',\'' + ret.data[i].name + '\',\'' + ret.data[i].price + '\',\'' + ret.data[i].images + '\')">';
						str = str + ' <img class="aui-img-object" src="' + ret.data[i].images + '" data-echo="' + ret.data[i].images + '">';
						str = str + '<div class="aui-img-body aui-text-left aui-ellipsis-2">';
						str = str + '<span class="aui-ellipsis-1">' + ret.data[i].name + '</span>';
						//						str = ' <span  class="aui-hot-ft14"><div class="ui-label-s">送</div> 越南红心火龙果一颗</span>';
						str = str + '<p><span class="goods-price aui-money">￥' + ret.data[i].price + '</span></p>';
						str = str + '</div>';
						str = str + '</li>';
						$api.append($api.byId('list'), str);
						//
						//					dataLoading = false;
						//					fnCacheImage(ret, 0);
					}
					api.hideProgress();
					api.refreshHeaderLoadDone();
				} else {
					api.alert({
						msg : JSON.stringify(err)
					});
				}
			});
		}
