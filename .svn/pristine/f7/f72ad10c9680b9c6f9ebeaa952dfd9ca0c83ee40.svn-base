/*background: #f7f7f7;*/
	background: rgba(247,247,247,0.7);
}
#aui-footer:before {
	content: "";
	position: absolute;
	top: 0px;
	left: 0px;
	right: 0px;
	border-top: 1px solid #ECF0F1;
	-webkit-transform: scaleY(.5);
	-webkit-transform-origin: 0 0;
}


/*水平线*/
.aui-line-x {
	clear:both;
	width: 100%;
	position: relative;
	height: 1px;
}
.aui-line-x:after {
	border-bottom: 1px solid #ECF0F1;
	display: block;
	content: '';
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	-webkit-transform-origin: 0 0;
	-webkit-transform: scale(1);
	pointer-events: none;
}
