/* 供应商管理 */
.html{
    background-color: #f0f0f0;
}
.article {
    font-size: 1.4rem;
    width: 100%;
}
/** 头部 */
.article .supplier {
    margin: 1rem 0rem 0rem -14.3rem;
    width: 28.4rem;
    font-size: 1.3rem;
}
.article .supplier div {
    width: 9.4rem;
}
.article .supplier a {
    width: 9.4rem;
    border-right: 0.1rem solid #0b923b;
}



/** 会员卡充值 */
.article .wrap {
    width: 100%;
    position: relative;
    top: 5.8rem;
    border-top: 0.1rem solid #eaeaea;
}
.article .wrap .choose {
    margin: 0.4rem 0rem 0rem 0rem;
    padding-bottom: 0.4rem;
    width: 100%;
    height: 3.6rem;
    line-height: 3.6rem;
    border-bottom: 0.1rem solid #eaeaea;
}
.article .wrap .choose label {
    width: 11rem;
    float: left;
    text-indent: 1rem;
}
.article .wrap .choose input {
    width: 20rem;
    height: 3.2rem;
    border: 0rem;
}
.article .wrap .choose select {
    padding: 0.4rem 0.6rem;
}
.article .wrap .choose input:focus {
    outline:none !important;
}
/*** 付款方式 */
.article .wrap .payment {
    width: 100%;
    height: 8.4rem;
}
.article .wrap .payment label {
    margin: 0rem 0rem 0rem 0rem;
    padding-top: 0.8rem;
    width: 100%;
    height: 3.4rem;
    line-height: 3.4rem;
    color: #b1b1b1;
    font-size: 1.6rem;
    text-indent: 1rem;
    background-color: #f0f0f0;
}
.article .wrap .payment .sort {
    width: 100%;
    height: 10rem;
    float: left;
    border-top: 0.1rem solid #eaeaea;
    border-bottom: 0.1rem solid #eaeaea;
    background-color: #ffffff;
}
.article .wrap .payment .sort div {
    margin-top: 2rem;
    width: 19.2%;
    height: 8rem;
    float: left;
}
.article .wrap .payment .sort div:nth-child(5) {
    width: 21%;
}
.article .wrap .payment .sort div img {
    margin-left: -2.1rem;
    width: 4rem;
    height: 4rem;
    position: relative;
    left: 50%;
    border: 0rem;
}
.article .wrap .payment .sort div span {
    width: 100%;
    text-align: center;
    font-size: 1.2rem;
}
.article .wrap .payment .sort div input {
    margin: 0.2rem 0rem 0rem -0.65rem;
    width: 1.3rem;
    height: 1.3rem;
    position: relative;
    left: 50%;
}
.article .wrap button {
    width: 100%;
    height: 4.3rem;
    text-align: center;
    line-height: 4.3rem;
    color: #ffffff;
    font-size: 1.6rem;
    position: fixed;
    bottom: 0rem;
    background-color: #0b923b;
}
/** 会员卡创建 */
.article .wrap .card_boutton {
    margin-bottom: 5.2rem;
}
/** 会员卡编辑 */
.article .wrap .inquiry input{
    width: 16rem;
}
.article .wrap .inquiry span {
    padding: 0.1rem 1rem;
    line-height: 2.6rem;
    border-radius: 0.4rem;
    border: 0.1rem solid #eaeaea;
}
/*** 间隔 */
.article .wrap .thread {
    margin: 0rem 0rem 0rem 0rem;
    padding-top: 0.8rem;
    width: 100%;
    height: 3.4rem;
    line-height: 3.4rem;
    color: #b1b1b1;
    font-size: 1.6rem;
    text-indent: 1rem;
    background-color: #f0f0f0;
}
/*** 列表 */
.article .list {
    padding-bottom: 6rem;
    border-top: 0.1rem solid #eaeaea;
}
.article .list ul {
    margin: 0rem;
    padding: 0rem;
    width: 100%;
    height: 7rem;
    line-height: 3rem;
    border-bottom: 0.1rem solid #eaeaea;
}
.article .list ul li {
    margin: 0.8rem 0rem -1rem 0rem;
    padding: 0rem;
    font-size: 1.2rem;
}
.article .list ul li label {
    padding: 0rem 0.2rem 0rem 0rem;
    float: left;
    text-indent: 1.18rem;
}
.article .list ul li span {
    float: left;
}
.article .list ul li:nth-child(1) {
    width: 38%;
    float: left;
    text-indent: 1.18rem;
}
.article .list ul li:nth-child(2),.list ul li:nth-child(3) {
    width: 31%;
    float: left;
}
.article .list ul li:nth-child(2) span,.list ul li:nth-child(3) span {
    width: 6.9rem;
    height: 3rem;
    overflow: hidden;
}
.article .list ul li:nth-child(4) {
     width: 100%;
     float: left;
}
.article .list ul li:nth-child(4) span {
    width: 31rem;
    height: 3rem;
    float: left;
    overflow: hidden;
}