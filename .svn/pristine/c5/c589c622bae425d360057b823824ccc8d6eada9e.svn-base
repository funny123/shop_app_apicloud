		    <style type="text/css">
		    body {
		        background: #ffffff;
		    }
		    .aui-content {
			    -webkit-overflow-scrolling: touch;
			    margin-bottom: 15px;
			    height: 100%;
			}
			.aui-flex-col{height:100%;}
			.aui-flex-item-3{height:100%;background:#f6f6f6;}
		    .box [class*=aui-flex-]:not(.aui-flex-col) {
		        background-color: rgba(26, 188, 156, 0.5);
		        color: #ffffff;
		        font-size: 12px;
		    }
		    .box.nobg [class*=aui-flex-]:not(.aui-flex-col){
		        background: #ffffff;
		    }
		    .box.bottom {
		        margin-bottom: 15px;
		    }
		    img {
		        width: 100%;
		    }
		    .aui-flex-item-9 {
			    float: left;
			    width: 75%;
			    padding: 12px 15px;
			}
		    .aui-border-t:after,
		    .aui-border-b:after,
		    .aui-border-l:after,
		    .aui-border-r:after,
		    .aui-border-tb:after,
		    .aui-border:after {
		        border-color: #e0e0e0;
		    }
		    .demo-box {
		    		height:550px;
		    		font-size:14px;
				overflow-y: auto;
		        -webkit-overflow-scrolling: touch;
		    }
		    .demo-box::-webkit-scrollbar {
		        width: 0px;
		        height: 0px;
		        visibility: hidden;
		        display: none;
		    }
		    .img-list {
		        padding: 2px;
		    }
		    .img-list > div {
		        padding: 2px;
		    }
		    .img-list img {
		        display: block;
		    }
		   .aui-list-view-cells {
			    position: relative;
			    padding: 12px 12px;
			    overflow: hidden;
			    -webkit-transform-style: preserve-3d;
			    transform-style: preserve-3d;
			    -webkit-touch-callout: none;
			}
			 .aui-list-view-cells:active {
					background: #f6f6f6;
					border-radius: 6px;
				}
			.aui-list-view-cell{background:#f6f6f6;}
			.active{border:0;		background:#fff;}
			.active:after {
			border-left: 5px solid #FF9800;
    			border-right: 1px solid #fff;   		
    			}    			
    			.aui-list-view:after {
   			 border-top: 0px solid #c8c7cc; }
		  </style>

